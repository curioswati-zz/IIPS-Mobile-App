.navbar.navbar-inverse.navbar-fixed-top(role='navigation')
  .container
    .navbar-header
      button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='.navbar-collapse')
        span.sr-only Toggle navigation
        span.icon-bar
        span.icon-bar
        span.icon-bar
      a.navbar-brand(href='#{prefix}') sequelize-compass
    .collapse.navbar-collapse
      ul.nav.navbar-nav

        // render navbar based on models list
        if modelsNames
          each modelName in modelsNames
            li
              a(href="#{prefix}/#{modelName.toLowerCase()}") #{modelName}